<% provide(:title, 'Sign up') %>
<h1>Sign up</h1>
<div class="row">
  <%= form_for(@user) do |f| %>
      <%=render 'shared/error_messages'%>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :email %>
      <%= f.text_field :email %>
      <%= f.label :password %>
      <%= f.text_field :password %>
      <%=f.label :password_confirmation,"Confirmation"%>
      <%=f.text_field :password_confirmation%>
      <%=f.submit "Create my account" %>
   <%end%>
 </div>